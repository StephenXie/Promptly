{
    "name": "Promptly",
    "description": "Supercharging your AI productivity with industry level prompting",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "http://*/*",
      "https://*/*",
      "https://deep-stable-gorilla.ngrok-free.app/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "http://*/*",
          "https://*/*"
        ],
        "js": ["content.js"]
      }
    ]
}