{
    "name": "Promptly",
    "description": "Supercharging your AI productivity with industry level prompting",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
      "activeTab"
    ],
    "host_permissions": [
      "https://chatgpt.com/*"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://chatgpt.com/*"
        ],
        "js": ["textboxedits.js", "getElementByXpath.js", "ui.js", "content.js"]
      }
    ]
}